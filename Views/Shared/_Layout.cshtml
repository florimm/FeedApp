<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - FeedApp</title>
    <base href="~/" />
    <style type="text/css">
        img {
            float: left;
            width: 100%;
            max-height: 260px;
            padding-bottom: 10px;
        }
        p {
            margin: 0 !important;
            float: left;
            width: 100%;
            color: #717171;
            font-size: 16px;
        }

    </style>

    <link rel="stylesheet" href="~/dist/vendor.css" asp-append-version="true" />
</head>
<body>
    @RenderBody()

    @RenderSection("scripts", required: false)
</body>
</html>
